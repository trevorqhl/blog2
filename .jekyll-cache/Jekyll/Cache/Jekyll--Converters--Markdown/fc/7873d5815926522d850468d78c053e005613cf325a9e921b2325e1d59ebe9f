<h1 id="checkout-repo">Checkout repo</h1>

<figure class="highlight"><pre><code class="language-ruby" data-lang="ruby"><span class="c1"># checkout master branch via ssh</span>
<span class="n">git</span> <span class="nb">clone</span> <span class="n">ssh</span><span class="ss">:/</span><span class="o">/</span><span class="n">git</span><span class="vi">@example</span><span class="p">.</span><span class="nf">com</span><span class="o">/</span><span class="n">project</span><span class="o">/</span><span class="n">hamon</span><span class="p">.</span><span class="nf">git</span>

<span class="c1"># checkout branch1 via https</span>
<span class="n">git</span> <span class="nb">clone</span> <span class="n">https</span><span class="ss">:/</span><span class="o">/</span><span class="n">trevorl</span><span class="vi">@example</span><span class="p">.</span><span class="nf">com</span><span class="o">/</span><span class="n">project</span><span class="o">/</span><span class="n">hamon</span><span class="p">.</span><span class="nf">git</span> <span class="o">-</span><span class="n">b</span> <span class="n">branch1</span>

<span class="c1"># switch to develop branch</span>
<span class="n">git</span> <span class="n">checkout</span> <span class="n">develop</span></code></pre></figure>

